{
  "name": "JSON viewer",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "Henry Heikkinen <rce@rce.fi>",
  "description": "The JSON viewer to use",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "*://*/*",
    "<all_urls>"
  ]
}